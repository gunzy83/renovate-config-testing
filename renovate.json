{
  "baseBranches": [
    "master"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "datasourceTemplate": "github-releases",
      "fileMatch": [
        ".*/Pulumi(\\.|\\.[a-z0-9-]*)yaml"
      ],
      "matchStrings": [
        "  [a-z][a-z.-]+:chart-version: [\"']?(?<currentValue>[^'\" ]+)[\"']? +# renovate: depName=(?<depName>[^ \\n]+) +(extractVersion=(?<extractVersion>[^\\n]+))?"
      ]
    }
  ],
  "enabledManagers": [
    "custom.regex"
  ],
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "matchDepNames": "gravitational/teleport",
      "allowedVersions": "/^[1-9][0-9]*\\.[1-9][0-9]*\\.[0-9]+$/"
    }
  ],
  "rebaseWhen": "conflicted"
}
