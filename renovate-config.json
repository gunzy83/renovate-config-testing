{
  "baseBranches": [
    "master"
  ],
  "rebaseWhen": "conflicted",
  "labels": [
    "dependencies"
  ],
  "automergeStrategy": "merge-commit",
  "ignoreTests": true,
  "customManagers": [
    {
      "customType": "regex",
      "datasourceTemplate": "github-releases",
      "fileMatch": [
        ".*/Pulumi(\\.|\\.[a-z0-9-]*)yaml"
      ],
      "matchStrings": [
        "  [a-z][a-z.]+:chart-version: [\"']?(?<currentValue>[^'\" ]+)[\"']? +# renovate: depName=(?<depName>[^ \\n]+) +(extractVersion=(?<extractVersion>[^\\n]+))?"
      ]
    }
  ]
}
